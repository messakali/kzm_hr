<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data>
        <record id="hr_payslip_employees_view_form_l10n_ma_payroll" model="ir.ui.view">
            <field name="name">hr.payslip.employees.form.l10n.ma.payroll</field>
            <field name="model">hr.payslip.employees</field>
            <field name="inherit_id" ref="hr_payroll.view_hr_payslip_by_employees" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='employee_ids']" position="attributes">
                    <attribute name="domain">[('is_contract_valid_by_context','in',(context.get('date_start',False),context.get('date_end',False),))]</attribute>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
