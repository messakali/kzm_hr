<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data noupdate="1">
<!-- Cotisation groups in the payslip-->
        <record id="cotisation_group_cnss" model="hr.cotisation.group">
            <field name="name">CNSS</field>
            <field name="code">CNSS</field>
            <field name="sequence" eval="1"/>
        </record>
        <record id="cotisation_group_tfp" model="hr.cotisation.group">
            <field name="name">Taxe de formation professionelle</field>
            <field name="code">TAX_FORMATION_PROFESSIONELLE</field>
            <field name="sequence" eval="2"/>
        </record>
        <record id="cotisation_group_pf" model="hr.cotisation.group">
            <field name="name">Prestation familiale</field>
            <field name="code">PRESTATION_FAMILIALE</field>
            <field name="sequence" eval="3"/>
        </record>
        <record id="cotisation_group_amo" model="hr.cotisation.group">
            <field name="name">AMO</field>
            <field name="code">AMO</field>
            <field name="sequence" eval="4"/>
        </record>
        <record id="cotisation_group_cimr" model="hr.cotisation.group">
            <field name="name">CIMR</field>
            <field name="code">CIMR</field>
            <field name="sequence" eval="5"/>
        </record>
        <record id="cotisation_group_assurance" model="hr.cotisation.group">
            <field name="name">Assurance retraite</field>
            <field name="code">ASSURANCE_RETRAITE</field>
            <field name="sequence" eval="6"/>
        </record>
<!-- Cotisation groups in the payroll ledger -->
        <record id="cotisation_ledger_cnss" model="hr.cotisation.ledger">
            <field name="name">CNSS</field>
            <field name="code">CNSS</field>
            <field name="sequence" eval="1"/>
        </record>
        <record id="cotisation_ledger_amo" model="hr.cotisation.ledger">
            <field name="name">AMO</field>
            <field name="code">AMO</field>
            <field name="sequence" eval="4"/>
        </record>
        <record id="cotisation_ledger_cimr" model="hr.cotisation.ledger">
            <field name="name">CIMR</field>
            <field name="code">CIMR</field>
            <field name="sequence" eval="5"/>
        </record>
        <record id="cotisation_ledger_assurance" model="hr.cotisation.ledger">
            <field name="name">Assurance retraite</field>
            <field name="code">ASSURANCE_RETRAITE</field>
            <field name="sequence" eval="6"/>
        </record>
<!-- Cotisations -->
        <record id="hr_cotisation_maladie_maternite" model="hr.cotisation">
            <field eval="1" name="sequence"/>
            <field name="code">MALADIE_MATERNITE</field>
            <field name="name">Maladie-maternité (prestations en espèces), décès</field>
            <field eval="0.67" name="rate_patronale"/>
            <field eval="0.33" name="rate_salariale"/>
            <field eval="6000" name="plafond_patronale"/>
            <field eval="6000" name="plafond_salariale"/>
            <field name="contribution_id" ref="contrib_register_cnss"/>
            <field model="account.account" name="account_credit" search="[('code','like','4441')]"/>
            <field name="group_id" ref="cotisation_group_cnss"/>
            <field name="ledger_id" ref="cotisation_ledger_cnss"/>
            <field name="type_cotisation">cnss</field>
        </record>
        <record id="hr_cotisation_pension" model="hr.cotisation">
            <field eval="2" name="sequence"/>
            <field name="code">PENSION</field>
            <field name="name">Pension</field>
            <field eval="7.93" name="rate_patronale"/>
            <field eval="3.96" name="rate_salariale"/>
            <field eval="6000" name="plafond_patronale"/>
            <field eval="6000" name="plafond_salariale"/>
            <field name="contribution_id" ref="contrib_register_cnss"/>
            <field model="account.account" name="account_credit" search="[('code','like','4441')]"/>
            <field name="group_id" ref="cotisation_group_cnss"/>
            <field name="ledger_id" ref="cotisation_ledger_cnss"/>
            <field name="type_cotisation">cnss</field>
        </record>
        <record id="hr_cotisation_prestation_familiale" model="hr.cotisation">
            <field eval="3" name="sequence"/>
            <field name="code">PRESTATION_FAMILIALE</field>
            <field name="name">Prestations familiales</field>
            <field eval="6.4" name="rate_patronale"/>
            <field name="contribution_id" ref="contrib_register_cnss"/>
            <field model="account.account" name="account_credit" search="[('code','like','4441')]"/>
            <field name="group_id" ref="cotisation_group_pf"/>
            <field name="ledger_id" ref="cotisation_ledger_cnss"/>
            <field name="type_cotisation">cnss</field>
        </record>
        <record id="hr_cotisation_amo_base" model="hr.cotisation">
            <field eval="4" name="sequence"/>
            <field name="code">AMO_BASE</field>
            <field name="name">AMO de base</field>
            <field eval="2" name="rate_patronale"/>
            <field eval="2" name="rate_salariale"/>
            <field name="contribution_id" ref="contrib_register_partner_mutuelle"/>
            <field model="account.account" name="account_credit" search="[('code','like','4445')]"/>
            <field name="group_id" ref="cotisation_group_amo"/>
            <field name="ledger_id" ref="cotisation_ledger_amo"/>
            <field name="type_cotisation">cnss</field>
        </record>
        <record id="hr_cotisation_amo_obligatoire_solidarite" model="hr.cotisation">
            <field eval="5" name="sequence"/>
            <field name="code">AMO_OBLIGATOIRE_SOLIDARITE</field>
            <field name="name">AMO obligatoire - Solidarité</field>
            <field eval="1.5" name="rate_patronale"/>
            <field name="contribution_id" ref="contrib_register_partner_mutuelle"/>
            <field model="account.account" name="account_credit" search="[('code','like','4445')]"/>
            <field name="group_id" ref="cotisation_group_amo"/>
            <field name="ledger_id" ref="cotisation_ledger_amo"/>
            <field name="type_cotisation">cnss</field>
        </record>
        <record id="hr_cotisation_indemnite_perte_emploi" model="hr.cotisation">
            <field eval="6" name="sequence"/>
            <field name="code">INDEMNITE_PERTE_EMPLOI</field>
            <field name="name">Indemnité pour perte d'emploi</field>
            <field eval="0.38" name="rate_patronale"/>
            <field eval="0.19" name="rate_salariale"/>
            <field eval="6000" name="plafond_patronale"/>
            <field eval="6000" name="plafond_salariale"/>
            <field name="contribution_id" ref="contrib_register_cnss"/>
            <field model="account.account" name="account_credit" search="[('code','like','4441')]"/>
            <field name="group_id" ref="cotisation_group_cnss"/>
            <field name="ledger_id" ref="cotisation_ledger_cnss"/>
            <field name="type_cotisation">cnss</field>
        </record>
        <record id="hr_cotisation_tax_formation_professionelle" model="hr.cotisation">
            <field eval="7" name="sequence"/>
            <field name="code">TAX_FORMATION_PROFESSIONELLE</field>
            <field name="name">Taxe de formation professionnelle</field>
            <field eval="1.6" name="rate_patronale"/>
            <field name="contribution_id" ref="contrib_register_cnss"/>
            <field model="account.account" name="account_credit" search="[('code','like','4441')]"/>
            <field name="group_id" ref="cotisation_group_tfp"/>
            <field name="ledger_id" ref="cotisation_ledger_cnss"/>
            <field name="type_cotisation">cnss</field>
        </record>
        <record id="hr_cotisation_cimr" model="hr.cotisation">
            <field eval="8" name="sequence"/>
            <field name="code">CIMR</field>
            <field name="name">CIMR</field>
            <field eval="3" name="rate_salariale"/>
            <field eval="3.9" name="rate_patronale"/>
            <field name="contribution_id" ref="contrib_register_cimr"/>
            <field model="account.account" name="account_credit" search="[('code','like','4443')]"/>
            <field name="group_id" ref="cotisation_group_cimr"/>
            <field name="ledger_id" ref="cotisation_ledger_cimr"/>
            <field name="type_cotisation">cimr</field>
        </record>
        <record id="hr_cotisation_assurance" model="hr.cotisation">
            <field eval="10" name="sequence"/>
            <field name="code">ASSURANCE_RETRAITE</field>
            <field name="name">Assurance retraite</field>
            <field eval="0" name="rate_salariale"/>
            <field eval="0" name="rate_patronale"/>
            <field name="contribution_id" ref="contrib_register_partner_assurance"/>
            <!-- <field model="account.account" name="account_credit" search="[('code','like','4443')]"/> -->
            <field name="group_id" ref="cotisation_group_assurance"/>
            <field name="ledger_id" ref="cotisation_ledger_assurance"/>
            <field name="type_cotisation">assurance</field>
        </record>
    </data>
</openerp>
