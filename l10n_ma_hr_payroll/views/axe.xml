<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <data>
        <record id="view_hr_axe_tree" model="ir.ui.view">
            <field name="name">hr.axe.tree</field>
            <field name="model">hr.axe</field>
            <field name="type">tree</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <tree string="Label">
                    <field name="cotisation_id" />
                    <field name="rubrique_id" />
                    <field name="avance_id" />
                    <field name="avantage_id" />
                    <field name="holiday_status_id" />
                    <field name="rule_parent_id" />
                    <field name="rule_salariale_id" />
                    <field name="rule_salariale_plafond_id" />
                    <field name="rule_patronale_id" />
                    <field name="rule_patronale_plafond_id" />
                </tree>
            </field>
        </record>
        <record id="view_hr_axe_form" model="ir.ui.view">
            <field name="name">hr.axe.form</field>
            <field name="model">hr.axe</field>
            <field name="type">form</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <form string="Axe">
                    <group colspan="4">
                        <field name="cotisation_id" />
                        <field name="rubrique_id" />
                        <field name="avance_id" />
                        <field name="avantage_id" />
                        <field name="holiday_status_id" />
                        <field name="rule_parent_id" />
                        <field name="rule_salariale_id" />
                        <field name="rule_salariale_plafond_id" />
                        <field name="rule_patronale_id" />
                        <field name="rule_patronale_plafond_id" />
                    </group>
                </form>
            </field>
        </record>
        <record id="view_hr_axe_search" model="ir.ui.view">
            <field name="name">hr.axe.search</field>
            <field name="model">hr.axe</field>
            <field name="type">search</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <search string="Axe">
                    <field name="cotisation_id" />
                    <field name="rubrique_id" />
                    <field name="avance_id" />
                    <field name="avantage_id" />
                    <field name="holiday_status_id" />
                </search>
            </field>
        </record>
        <record model="ir.actions.act_window" id="act_open_hr_axe_view">
            <field name="name">Axes</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">hr.axe</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_hr_axe_search" />
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_hr_axe_view_form">
            <field name="act_window_id" ref="act_open_hr_axe_view" />
            <field name="sequence" eval="20" />
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_hr_axe_form" />
        </record>
        <record model="ir.actions.act_window.view" id="act_open_hr_axe_view_tree">
            <field name="act_window_id" ref="act_open_hr_axe_view" />
            <field name="sequence" eval="10" />
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_hr_axe_tree" />
        </record>
        <menuitem id="menu_hr_axe" parent="hr_payroll.menu_hr_payroll_configuration" sequence="25" action="act_open_hr_axe_view" groups="kzm_base.group_unused" />
    </data>
</openerp>
