<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_hr_employee_qualification_tree" model="ir.ui.view">
            <field name="name">hr.employee.qualification.tree</field>
            <field name="model">hr.employee.qualification</field>
            <field name="type">tree</field>
            <field eval="8" name="priority"/>
            <field name="arch" type="xml">
                <tree string="Qualifications" editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="view_hr_employee_qualification_search" model="ir.ui.view">
            <field name="name">hr.employee.qualification.search</field>
            <field name="model">hr.employee.qualification</field>
            <field name="type">search</field>
            <field eval="8" name="priority"/>
            <field name="arch" type="xml">
                <search string="Qualifications">
                    <field name="name" select="True"/>
                </search>
            </field>
        </record>
        <record id="act_open_hr_employee_qualification_view"
            model="ir.actions.act_window">
            <field name="name">Qualifications</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">hr.employee.qualification</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="search_view_id" ref="view_hr_employee_qualification_search"/>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
        <record id="act_open_hr_employee_qualification_view_tree"
            model="ir.actions.act_window.view">
            <field name="act_window_id" ref="act_open_hr_employee_qualification_view"/>
            <field eval="10" name="sequence"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_hr_employee_qualification_tree"/>
        </record>
        <menuitem action="act_open_hr_employee_qualification_view"
            id="menu_hr_employee_qualification" parent="hr.menu_human_resources_configuration" sequence="12"/>
    </data>
</openerp>
