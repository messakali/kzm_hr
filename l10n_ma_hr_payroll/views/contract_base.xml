<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_hr_contract_base_tree" model="ir.ui.view">
            <field name="name">hr.contract.base.tree</field>
            <field name="model">hr.contract.base</field>
            <field name="type">tree</field>
            <field  eval="8" name="priority"/>
            <field name="arch" type="xml">
                <tree string="Type de salaire">
                    <field  name="code"/>
                    <field  name="name"/>
                </tree>
            </field>
        </record>
        <record id="view_hr_contract_base_form" model="ir.ui.view">
            <field name="name">hr.contract.base.form</field>
            <field name="model">hr.contract.base</field>
            <field name="type">form</field>
            <field  eval="8" name="priority"/>
            <field name="arch" type="xml">
                <form string="Type de salaire">
                    <group colspan="4">
                        <field  name="code"/>
                        <field  name="name"/>
                    </group>
                    <!-- <notebook colspan="4">
                        <page groups="base.group_multi_company" string="Sociétés">
                            <field  name="company_ids" nolabel="1"/>
                        </page>
                    </notebook> -->
                </form>
            </field>
        </record>
        <record id="view_hr_contract_base_search" model="ir.ui.view">
            <field name="name">hr.contract.base.search</field>
            <field name="model">hr.contract.base</field>
            <field name="type">search</field>
            <field  eval="8" name="priority"/>
            <field name="arch" type="xml">
                <search string="Type de salaire">
                    <field  name="code" select="True"/>
                    <field  name="name" select="True"/>
                </search>
            </field>
        </record>
        <record id="act_open_hr_contract_base_view" model="ir.actions.act_window">
            <field name="name">Type de salaire</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">hr.contract.base</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field  name="search_view_id" ref="view_hr_contract_base_search"/>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
        <record id="act_open_hr_contract_base_view_form" model="ir.actions.act_window.view">
            <field  name="act_window_id" ref="act_open_hr_contract_base_view"/>
            <field  eval="20" name="sequence"/>
            <field name="view_mode">form</field>
            <field  name="view_id" ref="view_hr_contract_base_form"/>
        </record>
        <record id="act_open_hr_contract_base_view_tree" model="ir.actions.act_window.view">
            <field  name="act_window_id" ref="act_open_hr_contract_base_view"/>
            <field  eval="10" name="sequence"/>
            <field name="view_mode">tree</field>
            <field  name="view_id" ref="view_hr_contract_base_tree"/>
        </record>
        <menuitem  action="act_open_hr_contract_base_view" id="menu_hr_contract_base" parent="menu_hr_configuration_contract" sequence="25"/>
    </data>
</openerp>
